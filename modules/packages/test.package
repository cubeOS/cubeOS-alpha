:package.test
DAT 0xffab, 0xcdff
DAT "test", 0

SET PUSH, C

SET A, 0
SET B, 0xA000 ;continuous memory location
JSR cat.open ;open root directory

SET B, POP
SET [B], 0xf001
ADD B, 1

SET A, 0xA005 ;first word in name
JSR typ.cCopy

SET [A], 0x000A ;LF (\n)
ADD A, 1
SET B, A

SET A, 0xA015 ;second link
JSR typ.cCopy

SET PC, POP
